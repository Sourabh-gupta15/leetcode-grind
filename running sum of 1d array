https://leetcode.com/problems/running-sum-of-1d-array/description/

class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        vector<int>ans;
        int i=0;int sum=0;
        while(i<nums.size()){
            sum=sum+nums[i];
            ans.push_back(sum);
            i++;
        }
        return ans;

    }
};
